CC = g++
CFLAGS = -c -Wall

all: isisCommandLineExample

isisCommandLineExample: isisCommandLineTest.o 
	$(CC) isisCommandLineTest.o -o isisCommandLineExample

isisCommandLineTest.o: isisCommandLineTest.cpp
	$(CC) $(CFLAGS) isisCommandLineTest.cpp

clean:
	rm -rf *.o isisCommandLineExample












